<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="315.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.NPContactViewAuctionsGUI">
   <children>
      <Pane prefHeight="398.0" prefWidth="315.0">
         <children>
            <ImageView fx:id="myLogoImageView" fitHeight="79.0" fitWidth="315.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../logo23.png" />
               </image>
            </ImageView>
            <Label fx:id="myUsernameLabel" layoutX="23.0" layoutY="71.0" text="User's Auctions:" />
            <Button fx:id="myBackBtn" layoutX="53.0" layoutY="363.0" mnemonicParsing="false" text="Back" />
            <Button fx:id="myCancelAuctionBtn" layoutX="108.0" layoutY="363.0" mnemonicParsing="false" text="Cancel Upcoming Auction" />
            <TableView fx:id="myAuctionTable" layoutX="21.0" layoutY="94.0" prefHeight="259.0" prefWidth="272.0">
              <columns>
                <TableColumn fx:id="myNameColumn" prefWidth="120.0" text="Auction Name" />
                <TableColumn fx:id="myDateColumn" prefWidth="86.0" text="Date" />
                  <TableColumn fx:id="myTimeColumn" prefWidth="65.0" text="Time" />
              </columns>
            </TableView>
         </children>
      </Pane>
   </children>
</AnchorPane>
