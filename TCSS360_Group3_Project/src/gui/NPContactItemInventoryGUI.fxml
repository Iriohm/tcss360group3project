<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.NPContactItemInventoryGUI">
   <children>
      <Pane prefHeight="433.0" prefWidth="601.0">
         <children>
            <ImageView fx:id="myLogoImageView" fitHeight="150.0" fitWidth="200.0" layoutX="199.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../logo23.png" />
               </image>
            </ImageView>
            <Text fx:id="myHeaderText" layoutX="30.0" layoutY="77.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Item Inventory for Auction: " />
            <Button fx:id="myAddItemBtn" layoutX="211.0" layoutY="374.0" mnemonicParsing="false" text="Add Item..." />
            <Button fx:id="myRemoveItemBtn" layoutX="303.0" layoutY="374.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="92.0" text="Remove item: " />
            <ChoiceBox fx:id="myItemChoice" layoutX="398.0" layoutY="374.0" prefHeight="25.0" prefWidth="48.0" />
            <Button fx:id="myBackBtn" layoutX="151.0" layoutY="374.0" mnemonicParsing="false" text="Back" />
            <Line endX="403.0" layoutX="198.0" layoutY="417.0" startX="-202.0" />
            <Label fx:id="myUsernameLabel" layoutX="2.0" layoutY="417.0" text="Label" />
            <TableView fx:id="myItemTableView" layoutX="27.0" layoutY="89.0" prefHeight="266.0" prefWidth="544.0">
              <columns>
                <TableColumn fx:id="myIDColumn" prefWidth="59.0" text="ID" />
                <TableColumn fx:id="myNameColumn" prefWidth="91.0" text="Item Name" />
                  <TableColumn fx:id="myConditionColumn" prefWidth="79.0" text="Condition" />
                  <TableColumn fx:id="myMinBidColumn" prefWidth="75.0" text="Min. Bid" />
                  <TableColumn fx:id="myQuantityColumn" prefWidth="65.0" text="Quantity" />
                  <TableColumn fx:id="mySizeColumn" prefWidth="62.0" text="Size" />
                  <TableColumn fx:id="myDescriptionColumn" prefWidth="112.0" text="Description" />
              </columns>
            </TableView>
         </children>
      </Pane>
   </children>
</AnchorPane>
